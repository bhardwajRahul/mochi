package main

import (
	"encoding/json"
	"fmt"
	"time"
)

func fact(n any) any {
	if (n == 0) {
		return 1
	}
	return (n * fact((n - 1)))
}

func main() {
	var n = 30
	var repeat = 1000
	var last = 0
	var start = time.Now().UnixNano()
	for i := 0; i < repeat; i++ {
		last = fact(n)
	}
	var duration = (((time.Now().UnixNano() - start)) / 1000000)
	var output = map[string]any{"duration_ms": duration, "output": last}
	func(){b,_:=json.Marshal(output);fmt.Println(string(b))}()
}

