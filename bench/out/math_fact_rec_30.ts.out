function fact(n) {
	if ((n == 0)) {
		return 1
	}
	return (n * fact((n - 1)))
}

function main(): void {
	let n = 30
	let repeat = 1000
	let last = 0
	let start = performance.now() * 1000000
	for (let i = 0; i < repeat; i++) {
		last = fact(n)
	}
	let duration = (((performance.now() * 1000000 - start)) / 1000)
	let output = {["duration_us"]: duration, ["output"]: last}
	console.log(JSON.stringify(output))
}
