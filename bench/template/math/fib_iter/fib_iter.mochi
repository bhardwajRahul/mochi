fun fib(n: int): int {
  var a = 0
  var b = 1
  for i in 0..n {
    let tmp = a + b
    a = b
    b = tmp
  }
  return a
}

// let n = 100
let n = {{ .N }}
let repeat = 1000
var last = 0

let start = now()
for i in 0..repeat {
  last = fib(n)
}
let duration = (now() - start) / 1000000

let output = {
  "duration_ms": duration,
  "output": last,
}
json(output)
