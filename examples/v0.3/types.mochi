// core/types.mochi
// User-defined types: Person and Book

/// Type: Person
type Person {
  name: string
  age: int
  email: string
}

/// Type: Book
type Book {
  title: string
  author: Person
  pages: int
  tags: list<string>
  metadata: map<string, string>
  published: bool
}

// Example variables using the custom types
let author: Person
let book: Book

print("custom types defined")
